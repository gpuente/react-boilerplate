{
  "name": "react-boiler",
  "version": "1.0.0",
  "description": "react boileplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack -p --config config/webpack.config.js",
    "build:development": "npm run clean && NODE_ENV=development webpack --config config/webpack.config.js",
    "clean": "rimraf dist",
    "server": "NODE_ENV=development webpack-dev-server --inline --hot --config config/webpack.config.js",
    "start": "NODE_ENV=development webpack-dev-server --inline --hot --config config/webpack.config.js",
    "server:production": "npm run build && http-server-spa dist",
    "deploy": "npm run build && surge --domain rspa.surge.sh -p dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "frontend",
    "boilerplate",
    "webpack",
    "mhr"
  ],
  "author": "Guillermo Puente <guillermo.ps09@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "colors": "^1.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server-spa": "^1.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "surge": "^0.19.0",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  }
}
